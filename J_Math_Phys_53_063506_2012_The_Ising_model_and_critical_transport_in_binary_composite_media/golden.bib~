  journal =	 "J. Appl. Phys.",
  year =	 "1977",
  volume =	 "48",
  number =  "12",
  pages =	 "5301-5303",
%  note =  "An approximate formula for the critical volume fraction 
%          for percolation
%            of the small metal spheres 
%           in a compressed powder is given by
%        $p_c=(1+\xi \phi / (4 x_c))^{-1}$, where $\phi$
%         is a reciprocal planar packing factor, and $x_c$
%         is a critical surface area fraction of the
%         larger particles which must be covered
%         for percolation by the smaller particles.
%         Values based on microstructural analysis 
%         giving good agreement with conductivity 
%         experiments are
%         $x_c=0.42$ and $\phi=1.27$, which we have also
%         used as a reasonable approximation for sea ice.",
}



@Unpublished{Kusy:formula,
  author =       "  ",
  title =        " ",
  note =  "The critical volume fraction for percolation
            of the small metal spheres 
           in a compressed powder is given by
        $p_c=(1+\xi \phi / (4 x_c))^{-1}$, where $\phi$
         is a reciprocal planar packing factor, and $x_c$
         is the critical surface area fraction of the
         large polymer particles which must be covered
         for percolation of the smaller metal particles.
          Values based on microstructural analysis which
         gave good agreement with experiment are
         $x_c=0.42$ and $\phi=1.27$, which we have also
          used as a reasonable approximation for sea ice.",
}









